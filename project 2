.data
    data: .space 1001
    output: .asciiz "\n"
    notvalid: .asciiz "Invalid input"
.text

main:
  #asks for input and stores it

    li $v0, 8
    la $a0, data
    li $a1, 1001
    syscall
    jal begin
    j finish
begin:
la $t0,data
    add $t0,$t0,$t1
    lb $s0, ($t0)
    beq $s0, 0, convert
    beq $s0, 9, skip
    beq $s0, 32, skip
    move $t6, $t1
    j doing

skip:
